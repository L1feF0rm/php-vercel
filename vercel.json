{
  "name": "Experimental PHP and static deployment",
  "functions": {
    "api/**/*.php": {
      "runtime": "vercel-php@0.5.1"
    }
  },
  "routes": [{
    "src": "/(.*).html",
    "dest": "publics/$1.html", 
    "methods": ["GET"]
  }, {
    "src": "/(css|js)/(.*)",
    "dest": "publics/$1/$2",
    "methods": ["GET"]
  }, {
    "src": "/(.*)",
    "dest": "/api/index.php"
  }]
}