{
  "name": "Experimental PHP and static deployment",
  "functions": {
    "api/**/*.php": {
      "runtime": "vercel-php@0.5.1"
    }
  },
  "routes": [{
    "src": "/virtualHtml(.*).html",
    // "dest": "public/$1.html", 
    "methods": ["GET"]
  }, {
    "src": "/virtualJs/(css|js)/(.*)",
    // "dest": "public/$1/$2",
    "methods": ["GET"]
  }, {
    "src": "/(.*)",
    "dest": "/api/index.php"
  }]
}