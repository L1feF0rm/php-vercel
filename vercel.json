{
  "name": "Experimental PHP and static deployment",
  "functions": {
    "api/**/*.php": {
      "runtime": "vercel-php@0.5.1"
    }
  },
  "routes": [{
    "src": "/(.*).html",
    "dest": "public/$1.html"
  }, {
    "src": "/(css|js)/(.*)",
    "dest": "public/$1/$2"
  }, {
    "src": "/(.*)",
    "dest": "/api/index.php"
  }]
}